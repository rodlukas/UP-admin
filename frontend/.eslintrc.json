{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["import", "flowtype", "jsx-a11y", "react", "react-hooks", "prettier"],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {}
}
