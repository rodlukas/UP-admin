    "audit-ci": "^3.1.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "confusing-browser-globals": "^1.0.9",
    "cross-env": "^7.0.0",
